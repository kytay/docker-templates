<?xml version="1.0" encoding="UTF-8"?>
<Container>
	<TemplateURL>https://raw.githubusercontent.com/kytay/docker-templates/master/nextcloud.xml</TemplateURL>
	<Date>2020-03-15</Date>
	<Name>kytay/nextcloud</Name>
	<Repository>nextcloud</Repository>
	<Project>https://nextcloud.com</Project>
	<Registry>https://registry.hub.docker.com/library/nextcloud</Registry>
	<Network>bridge</Network>
	<Privileged>false</Privileged>
	<WebUI>https://[IP]:[PORT:443]/</WebUI>
	<Icon>https://d1q6f0aelx0por.cloudfront.net/product-logos/library-nextcloud-logo.png
	</Icon>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>443</HostPort>
				<ContainerPort>443</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
	<Data>
		<Volume>
			<HostDir></HostDir>
			<ContainerDir>/var/www/html</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
	</Data>
	<Environment>
		<Variable>
			<Value>99</Value>
			<Name>PUID</Name>
		</Variable>
		<Variable>
			<Value>100</Value>
			<Name>PGID</Name>
		</Variable>
		<Variable>
			<Value></Value>
			<Name>MYSQL_DATABASE</Name>
		</Variable>
		<Variable>
			<Value></Value>
			<Name>MYSQL_USER</Name>
		</Variable>
		<Variable>
			<Value></Value>
			<Name>MYSQL_PASSWORD</Name>
		</Variable>
		<Variable>
			<Value></Value>
			<Name>MYSQL_HOST</Name>
		</Variable>
	</Environment>
</Container>